<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kaizen.hoymm.compassnetguru.MainActivity"
    android:paddingTop="@dimen/AppPaddingTop"
    android:paddingRight="@dimen/AppPaddingRight"
    android:paddingEnd="@dimen/AppPaddingRight"
    android:paddingBottom="@dimen/AppPaddingBottom"
    android:paddingLeft="@dimen/AppPaddingLeft"
    android:paddingStart="@dimen/AppPaddingLeft"
    >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/compassFrame"
        />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/buttonsFrame"
        />

</android.support.constraint.ConstraintLayout>
